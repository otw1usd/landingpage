  <script>
    console.log('mulai');


    fetch('data.json')
      .then(response => response.json())
      .then(function(response) {
        const datascene = response.scenes;
        let sceneName = '';
        datascene.forEach(m => {
          sceneName += `<a href="javascript:void(0)"class="scene btn btn-outline-dark btn-sm btn-block button-utama" data-id="${m.id}"><li class="text">${m.name}</li></a>`;
        });
        const isidatasceneName = document.querySelector('.scenes');
        isidatasceneName.innerHTML = sceneName;
      })

      .then(response => {
        let script = document.createElement('script');
        script.src = 'dataset/360/data.js';
        document.head.appendChild(script);
      })

  </script>


