  <!-- info-container -->
  <div class="info-container col-2 bg-dark">

  </div>


  <!-- List of Own Project -->
  <div class="div-daftar-proyek col-9">
    <h2>Project List</h2>
    <h4>Contact <a href="/#contactUs">Admin AMATI</a> for more information </h4>
    <a href="/registerproject"><img class="col-1" id="icon-edit-data-proyek" src="images/tambah.jpg" alt="Create New Project"> </a>

    <% listprojects.forEach((listproject) => { %>
    <%    for (i=0 ; i < listproject.length ; i++) { %>

    <div class="daftar-proyek col-12 row">
      <div class="col-2">
        <img class="logo-proyek" src="images/logo-kcic.png" alt="">
      </div>
      <div class="div-data-proyek-besar col-10">
        <div class="row">
          <h4>
            <a href="/project/<%= listproject[i]._id %>" class="nama-proyek text-dark">
              <%= listproject[i].projectName %>
            </a>
          </h4>
        </div>
        <div class="row">
          <div class="div-data-proyek col-4">
            <p class="data-proyek">Project Duration: <%= listproject[i].startDate %> until <%= listproject[i].endDate %></p>
            <p class="data-proyek">Locaton: <%= listproject[i].location %></p>
            <p class="data-proyek">Area: </p>
          </div>
          <div class="div-data-proyek col-3">
            <p class="data-proyek">Project Cost: <%= listproject[i].nilaiProyek%></p>
            <p class="data-proyek">Owner: <%= listproject[i].username %></p>
            <p class="data-proyek">Consultant: <%= listproject[i].konsultan%></p>
          </div>
          <div class="div-data-proyek col-2">
            <p class="data-proyek">Contractor: <%= listproject[i].kontraktor%></p>
            <p class="data-proyek"></p>
          </div>
          <div class="div-data-proyek col-3">
            <!-- <p class="data-proyek">Your Position: <%= jobs %></p> -->
            <a href="/project/<%= listproject[i]._id %>" class="btn btn-warning btn-sm"> Notification</a>
            <a href="/project/<%= listproject[i]._id %>" class="btn btn-warning btn-sm"> Link Invite</a>
          </div>
        </div>
      </div>
    </div>
    <% } %>
    <% }) %>
  </div>
