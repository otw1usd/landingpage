  <section id="home" class="hero-section">
    <div class="hero-section__bg-image"></div>

    <div class="container hero-section__content">
      <div class="row row-cols-lg-2">
        <div class="hero-section__text">
          <h1 class="page__title">AMATI</h1>
          <p class="section__desc text-white mb-0">
            A visualization platform for all your projects. <br>
            Capture, upload, observe.
          </p>
          <a href="#about" class="btn btn-primary btn-sm mt-4">Read More</a>
        </div>
        <div class="hero-section__form position-relative">
          <div class="form-container bg-white p-4 rounded-border shadow-lg">
            <%- include('./partials/messages.ejs') %>
            <h3 class="heading-3 primary-text mt-2 made-tommy-soft-bold">Sign Up Now</h3>
            <p class="body__text">Enjoy our feature right now!</p>
            <h4 class="sign-up-parts primary-text">Personal Information</h4>
            <form action="/" method="POST">
              <div class="form-group mb-3">
                <img width="18" height="20" src="/images/user.svg" alt="User Icon" />
                <input type="text" class="form-control" id="name" aria-describedby="nameHelp" placeholder="Name" name="name" value="<%= typeof name != 'undefined' ? name : '' %>" required />
              </div>
              <div class="form-group mb-3">
                <img width="18" height="20" src="/images/user.svg" alt="Username Icon" />
                <input type="text" class="form-control" id="username" aria-describedby="usernameHelp" placeholder="Username" name="username" value="<%= typeof username != 'undefined' ? username : '' %>" required />
              </div>
              <div class="form-group mb-3">
                <img width="21" height="15" src="/images/email.svg" alt="Email Icon" />
                <input type="email" class="form-control" id="email" aria-describedby="useremailHelp" placeholder="Email" name="email" value="<%= typeof email != 'undefined' ? email : '' %>" required />
              </div>
              <div class="form-group mb-3">
                <img width="15" height="20" src="/images/lock.svg" alt="Password Icon" />
                <input type="password" class="form-control" id="password" placeholder="Password" name="password" value="<%= typeof password != 'undefined' ? password : '' %>" required />
              </div>
              <div class="form-group mb-3">
                <img width="22" height="22" src="/images/bullseye.svg" alt="Phone Number Icon" />
                <input type="text" class="form-control" id="nohp" aria-describedby="usernomorHPHelp" placeholder="Phone Number" name="nohp" value="<%= typeof nohp != 'undefined' ? nohp : '' %>" required />
              </div>
              <h4 class="sign-up-parts primary-text mb-3">
                Work Status and Experience
              </h4>
              <div class="form-group mb-3">
                <img width="20" height="16" src="/images/briefcase.svg" alt="Company Icon" />
                <input type="text" class="form-control" id="company" aria-describedby="usercompanyflarnaHPHelp" placeholder="Company" name="company" value="<%= typeof company != 'undefined' ? company : '' %>" required />
              </div>
              <div class="form-group mb-3">
                <img width="23" height="23" src="/images/sticky-notes.svg" alt="Position Icon" />
                <input type="text" class="form-control" id="job" aria-describedby="userpositionHPHelp" placeholder="Position" name="jobs" value="<%= typeof jobs != 'undefined' ? jobs : '' %>" required />
              </div>
              <div class="
                      d-flex
                      flex-column flex-md-row
                      justify-content-between
                      align-items-baseline
                      row
                      submit-and-have-account
                    ">

                <button type="submit" class="btn btn-sm btn-primary order-0 col-12 sign-up-form-submit">
                  Submit
                </button>
                <p class="body__text m-0 order-1 mb-4 mb-md-0 have-account-text col-12">
                  Already have an account?
                  <a href="/login" class="have-account-log-in">Log in</a>
                </p>
              </div>
            </form>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- Overview of the Construction Industry -->
  <section id="about" class="construction">
    <div class="row row-cols-lg-2">
      <div class="col-md-6">
