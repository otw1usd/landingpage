<form class="sign-up-form" action="/admin?_method=PUT" method="POST">
  <h2 class="sign-up-heading">Edit Project</h2>
  
  <input type="hidden" name="_id" value="<%= projectnow._id %>" />
  <input type="hidden" name="oldprojectUsername" value="<%= projectnow.oldprojectUsername || projectnow.projectUsername  %>" />

  <h3 class="sign-up-sub-heading"></h3>
  <label for="projectName">Project Name:</label>
  <input class="sign-up-box" type="text" name="projectName" placeholder="Nama Project" id="projectName" value="<%= projectnow.projectName  %>" required/><br>

  <label for="location">Project Location:</label>
  <input class="sign-up-box" type="text" name="location" placeholder="Location" id="location" value="<%= projectnow.location  %>" required/><br>
  
  <label for="projectDescription">Project Description:</label>
  <input class="sign-up-box" type="text" name="projectDescription" placeholder="Deskripsi Project" id="projectDescription" value="<%= projectnow.projectDescription  %>"  required/><br>
  
  <label for="startDate">Project Start Date:</label>
  <input class="sign-up-box" type="date" name="startDate" placeholder="Waktu Mulai" id="startDate" value="<%= projectnow.startDate  %>" required/><br>

  <label for="endDate">Project End Date:</label>
  <input class="sign-up-box" type="date" name="endDate" placeholder="Waktu Selesai" id="endDate" value="<%= projectnow.endDate  %>" required/><br>

  <label for="projectUsername">Project Username:</label>
  <input class="sign-up-box" type="text" name="projectUsername" placeholder="username project" id="projectUsername" value="<%= projectnow.projectUsername  %>" required/><br>

  <label for="projectPassword">Project Password:</label>
  <input class="sign-up-box" type="text" name="projectPassword" placeholder="password project" id="projectPassword" value="<%= projectnow.projectPassword  %>" required/><br>

  <label for="username">Project Owner username: </label>
  <input class="sign-up-box" type="text" name="username" placeholder="username owner" id="username" value="<%= projectnow.username  %>" required/><br>
  
  <label for="progrestotal">Project Progress Total: </label>
  <input class="sign-up-box" type="text" name="progrestotal" placeholder="dalam persen" id="progrestotal" value="<%= projectnow.progrestotal  %>" required/><br>

  <div class="form-check">
    <input class="form-check-input" type="checkbox" value="Bulanan" id="timestampproject1" name="timestampproject1">
    <label class="form-check-label" for="timestampproject1">
      Bulanan
    </label>
  </div>

  <div class="form-check">
    <input class="form-check-input" type="checkbox" value="Mingguan" id="timestampproject1" name="timestampproject1">
    <label class="form-check-label" for="timestampproject1">
      Mingguan
    </label>
  </div>

  
  <button type="submit" class="btn btn-dark">Ubah Data Project</button>
  
</form>